{
	"apps": [{
		"name": "hubot-natual",
		"interpreter": "/bin/bash",
		"watch": true,
		"ignore_watch" : ["client/img"],
		"script": "bin/hubot",
		"args": "-a rocketchat",
		"port": "3030",
		"env": {
			"ROCKETCHAT_URL": "https://my.rocket.chat",
			"ROCKETCHAT_ROOM": "natural-bots",
			"RESPOND_TO_DM": true,
			"ROCKETCHAT_USER": "$botuser",
			"ROCKETCHAT_PASSWORD": "$botuserpass",
			"ROCKETCHAT_AUTH": "password",
			"HUBOT_LOG_LEVEL": "debug"
		}
	}]
}
